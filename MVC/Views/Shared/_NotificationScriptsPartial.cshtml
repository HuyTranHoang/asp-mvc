@model string

@{
    var message = Html.Raw(Json.Serialize(Model));
}

@if (!string.IsNullOrEmpty(Model))
{
    <script >
        /*global Swal */
        Swal.fire({
          icon: 'success',
          title: 'Successfully!',
          text: '@Model',
          timer: 1500
        })
    </script>
}